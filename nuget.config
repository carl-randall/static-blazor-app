<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="Carl Randall Github Package Source" value="https://nuget.pkg.github.com/carl-randall/index.json" />		
	</packageSources>
	<packageRestore>
		<add key="enabled" value="True" />
		<add key="automatic" value="True" />
	</packageRestore>
	<bindingRedirects>
		<add key="skip" value="False" />
	</bindingRedirects>
	<packageManagement>
		<add key="format" value="0" />
		<add key="disabled" value="False" />https://github.com/carl-randall/static-blazor-app/blob/test-nuget-config/nuget.config#L19C38
	</packageManagement>	
	<packageSourceCredentials>		
		<Carl_Randall_Github_Package_Source>
			<add key="Username" value="%GITHUB_ACTOR%" />
      			<add key="ClearTextPassword" value="%GIT_PACKAGES%" />
		</Carl_Randall_Github_Package_Source>		
	</packageSourceCredentials>
</configuration>
